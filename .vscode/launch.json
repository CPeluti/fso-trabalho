{
  "version": "0.2.0",
  "configurations": [
  {
    "name": "(gdb) Anexar",
    "type": "cppdbg",
    "request": "attach",
    "program": "insira o nome do programa, por exemplo ${workspaceFolder}/a.exe",
    "MIMode": "gdb",
    "miDebuggerPath": "/path/to/gdb",
    "setupCommands": [
        {
            "description": "Habilitar a reformatação automática para gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        {
            "description": "Definir Tipo de Desmontagem como Intel",
            "text": "-gdb-set disassembly-flavor intel",
            "ignoreFailures": true
        }
    ]
  },
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
      "args": [
        "${workspaceFolder}\\processes.txt",
        "${workspaceFolder}\\files.txt"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: g++.exe arquivo de build ativo"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/mnt/c/Users/DELL/OneDrive - unb.br/07 Sétimo Semestre/Fundamentos de Sistemas Operacionais/fso-trabalho/src",
      "program": "/mnt/c/Users/DELL/OneDrive - unb.br/07 Sétimo Semestre/Fundamentos de Sistemas Operacionais/fso-trabalho/src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}